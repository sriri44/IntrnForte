.filter-item.selected:focus:not(:focus-visible), .filter-item[aria-selected="true"]:focus:not(:focus-visible), .filter-item[aria-current]:not([aria-current="false"]):focus:not(:focus-visible) { outline: transparent solid 1px; box-shadow: none; }

